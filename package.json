    "@babel/preset-env": "^7.9.6",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.5"
  },
